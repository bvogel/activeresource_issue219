<p id="notice"><%= notice %></p>

<h1>Listing Test Objects</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_objects.each do |test_object| %>
      <tr>
        <td><%= test_object.value %></td>
        <td><%= link_to 'Show', test_object %></td>
        <td><%= link_to 'Edit', edit_test_object_path(test_object) %></td>
        <td><%= link_to 'Destroy', test_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test object', new_test_object_path %>
